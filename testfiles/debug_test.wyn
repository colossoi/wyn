-- Test file for GDP debug output
-- Exercises debug_i32, debug_f32, and debug_str intrinsics

#[uniform(set=1, binding=0)] def iResolution: vec2f32

#[fragment] def fragment_main(#[location(0)] fragCoord: vec2f32) -> #[location(0)] vec4f32 =
  let _ = debug_str("hello") in
  let _ = debug_i32(42) in
  let _ = debug_f32(3.14159f32) in
  let x = fragCoord.x / iResolution.x in
  let _ = debug_f32(x) in
  @[x, 0.0, 0.0, 1.0]
